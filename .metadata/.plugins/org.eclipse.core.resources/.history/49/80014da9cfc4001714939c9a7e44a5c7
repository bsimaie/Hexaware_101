package demos;

public class ForLoops {

	public static void main(String[] args) {
		int n = 5;
		int m = 6;
		printMultiplicationTable(n, m);
	}
	
	private static void printMultiplicationTable(int n, int m) {
		for(int i = 1; i <= n; i++) {
			for (int j = 1; j <= m; j++) {
				System.out.println(i + " x " + n + " = " + i*n);
			}
			
		}
	}

}
