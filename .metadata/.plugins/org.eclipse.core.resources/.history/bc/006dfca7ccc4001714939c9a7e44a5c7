package demos;

public class Loops {

	public static void main(String[] args) {
		int n = 5;
		printNumbers(n);
	}
	
	private static void printNumbers(int index) {
		int i = 1;
		do {
			System.out.println(i);
			i = i+1;
		} while (i <= index);
	}
	
	private static void sumNumbers(int index) {
		int i = 1;
	}

}
