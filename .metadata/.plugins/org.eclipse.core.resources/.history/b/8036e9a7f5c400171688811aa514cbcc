	@Test
	public void mySecondTest() {
		System.out.println("My Second TestNG Test");
	}
	
	@Test
	public void myTest() {
		System.out.println("My TestNG Test");
	}
	
	@BeforeMethod
	public void setUp() {
		System.out.println("Setup");
	}
	
	@AfterMethod
	public void tearDown() {
		System.out.println("Teardown");
	}

}
